type=cson pathLocal=.atom/config.cson pathRemote=.mods.d/dotfiles/atom-config.cson ignore=atom-ignore
type=cson pathLocal=.atom/keymap.cson pathRemote=.mods.d/dotfiles/atom-keymap.cson
type=gitconf pathLocal=.gitconfig pathRemote=.mods.d/dotfiles/gitconfig ignore=git-ignore
